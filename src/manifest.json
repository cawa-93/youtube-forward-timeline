{
  "manifest_version": 2,
  "name": "YouTube Forward Timeline",
  "version": "0.0.1",
  "description": "Adds a fast forward button to the next section",
  "author": "Alex Kozack",
  "homepage_url": "https://github.com/cawa-93/youtube-forward-timeline",
  "icons": {
    "373": "YouTubeRewind.png"
  },
  "applications": {
    "gecko": {
      "id": "kozackunisoft@gmail.com",
      "strict_min_version": "52.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/watch*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
